<packageInfo>
    <bundleId>org.digitalknightz.dockplus</bundleId>
    <name>Dock+</name>
    <version>0.6-1</version>
    <descriptions>
        <description>
		Customize your dock to your liking!
        </description>
	<description>
		Dock+ gives you the ability to add more apps to your dock. It also allows you to create dock pages. Just add apps 
		to another dock page in the same way that you would for springboard pages.
    	</description>
    </descriptions>
    <compatibility>
        <miniOS>12.0</miniOS>
        <maxiOS>12.1.4</maxiOS>
    </compatibility>
    <dependencies>
        <dependency>mobilesubstrate</dependency>
    </dependencies>
    <changelog>
        <change>
            <changeVersion>0.6-1 </changeVersion>
            <changeDescription>Initial Release</changeDescription>
        </change>
    </changelog>
</packageInfo>
